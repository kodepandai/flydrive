{
  "name": "root",
  "private": true,
  "scripts": {
    "build": "npm run clean && NODE_ENV=production rollup -c",
    "clean": "rm -rf packages/flydrive/build & rm -rf packages/flydrive-s3/build & rm -rf packages/flydrive-gcs/build",
    "lint": "prettier --check --plugin-search-dir=. . && eslint . --fix",
    "prettier": "prettier --write --plugin-search-dir=. .",
    "format": "npm run prettier && npm run lint",
    "version:bump": "node version.mjs",
    "sync": "syncpack fix-mismatches"
  },
  "devDependencies": {
    "@lunoxjs/build": "2.0.0-next",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "prettier": "^2.5.1",
    "rollup": "^3.25.2",
    "rollup-plugin-delete": "^2.0.0",
    "syncpack": "^10.9.3",
    "ts-node": "^9.1.1",
    "tslib": "^2.3.1",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  },
  "volta": {
    "node": "15.10.0"
  }
}
