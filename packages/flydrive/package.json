{
  "name": "@kodepandai/flydrive",
  "version": "3.0.0-next.4",
  "description": "Flexible and Fluent way to manage storage in Node.js.",
  "homepage": "https://github.com/KodePandai/flydrive#readme",
  "license": "MIT",
  "main": "dist/cjs/index.cjs",
  "types": "dist/esm/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts",
      "require": "./dist/index.js"
    },
    "./exceptions/*": {
      "import": "./dist/exceptions/index.mjs",
      "types": "./dist/exceptions/index.d.mts",
      "require": "./dist/exceptions/index.js"
    },
    "./dist/*": "./dist/*"
  },
  "keywords": [
    "storage",
    "filesystem",
    "file",
    "aws",
    "s3",
    "promise",
    "async",
    "spaces",
    "google",
    "cloud"
  ],
  "author": "Romain Lanz <romain.lanz@slynova.ch>",
  "contributors": [
    "Akhmad Salafudin <axmad386@gmail.com>",
    "Harminder Virk <virk@adonisjs.com>",
    "MichaÃ«l Zasso <targos@pm.me>"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest"
  },
  "dependencies": {
    "fs-extra": "^9.1.0",
    "node-exceptions": "^4.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KodePandai/flydrive.git"
  },
  "bugs": {
    "url": "https://github.com/KodePandai/flydrive/issues"
  }
}
